<h1>Add a new pet</h1>
<p>Find me in app/views/pets/new.html.erb</p>
<%= simple_form_for(@pet) do |f| %>
  <%= f.input :name %>
  <%= f.input :pet_type %>
  <%= f.input :breed %>
  <%= f.input :bio %>
  <%= f.input :photo, as: :file %>
  <%# <%= f.input :photo, as: :file %>
  <%= f.button :submit %>
<% end %>
