<h1>Edit your play date</h1>
<div class="btn btn-outline-success index-button"><%= link_to "Delete", play_date_path(@play_date), data: {turbo_method: :delete, turbo_confirm: "Are you sure?"} %></div>
<%= simple_form_for [@pet, @play_date] do |f| %>
  <%= f.input :start_time %>
  <%= f.input :end_time %>
  <%= f.button :submit %>
<% end %>
